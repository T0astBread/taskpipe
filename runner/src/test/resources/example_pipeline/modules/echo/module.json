{
	"name": "echo",
	"description": "Echoes text into a file",
	"operation_modes": "individual|group",
	"parameters": [
		{
			"name": "text",
			"description": "The text to echo"
		},
		{
			"name": "target",
			"description": "The path of the file to echo into, relative to the entry's root"
		}
	],
	"run_command": {
		"linux": "{module_dir}/echo.sh \"{text}\" \"{target}\""
   }
}
